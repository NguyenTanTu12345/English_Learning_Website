
@{
    ViewBag.Title = "Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Result</h2>
@{ 
    List<bool> vs = Session["save_Result"] as List<bool>;
    List<int> a = new List<int>();
    for (int i = 1; i < 11; i ++)
    {
        a.Add(i);
    }
}
<table class="table-hover table table-responsive tr">
    <caption class="caption">Result</caption>
    <tr>
        <th>Number</th>
        <th>Result</th>
    </tr>
    @foreach (var item in vs)
    {
        foreach (var number in a)
        {
            <tr>
                <td>@number</td>
                @if (item == true)
                {
                    <td style="color:greenyellow;">V</td>
                }
                else
                {
                    <td style="color:red">X</td>
                }
            </tr>
            a.Remove(number);
            break;
        }

    }
</table>

